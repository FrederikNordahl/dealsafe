{
  "expo": {
    "name": "dealsafe",
    "slug": "dealsafe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dealsafe",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIStatusBarStyle": "UIStatusBarStyleLightContent",
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationsUsageDescription": "We'll remind you when your vouchers are about to expire.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "dealsafe"
            ]
          }
        ]
      },
      "bundleIdentifier": "com.desperate.dealsafe",
      "associatedDomains": [
        "applinks:dealsafe.app"
      ],
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.desperate.dealsafe"
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.desperate.dealsafe"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1D1D1F"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow DealSafe to access your photos to attach them to deals.",
          "cameraPermission": "Allow DealSafe to use your camera to take photos of deals."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow DealSafe to access your photo library.",
          "savePhotosPermission": "Allow DealSafe to save photos to your library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-document-picker"
      ],
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsFileWithMaxCount": 10,
            "NSExtensionActivationSupportsImageWithMaxCount": 10
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#010101",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1a59711e-8421-404f-9ea1-0f4a71dbf242"
      }
    },
    "owner": "frederiknordahl"
  }
}
